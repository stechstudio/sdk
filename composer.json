{
    "name": "stechstudio/sdk",
    "require": {
        "guzzlehttp/guzzle": "^6.1",
        "illuminate/validation": "^5.4|^6.0",
        "illuminate/pipeline": "^5.4|^6.0",
        "ramsey/uuid": "^3.1",
        "tedivm/stash": "0.14.*",
        "psr/log": "^1.0",
        "symfony/debug": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "dev-master",
        "laravel/helpers": "^1.1"
    },
    "authors": [
        {
            "name": "Joseph Szobody",
            "email": "joseph@stechstudio.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "STS\\Sdk\\": "src/"
        },
        "files" : [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/TestCase.php"
        ]
    }
}
